<!DOCTYPE html>
<html>
<head>
  <title>Greed</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<% unless flash.empty? %>
<% flash.keys.each do |key| %>
<p id="<%= key %>">
  <%= flash[key] %>
</p>
<% end %>
<% end %>

<%= yield %>

</body>
</html>
